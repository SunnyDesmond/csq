.settings-wrap {
  margin-top: 20rpx;
  background-color: #fff;
}

.settings-wrap .cell {
  height: 90rpx;
  border-bottom: 1rpx solid #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  background: url(data:image/png;base64,) no-repeat center right 30rpx/13rpx 22rpx;
}

.settings-wrap .cell:active {
  background-color: #f5f5f5;
}


.settings-wrap .cell .title {
  font-size: 28rpx;
  color: #393939;
  margin-left: 25rpx;
}

.settings-wrap .cell .value{
  font-size: 24rpx;
  color: #b7b7b7;
  padding-right: 40rpx;
}