page {
  height: 100%;
  background-color: #f5f5f5;
}

.top-wrap2{
  height: 204rpx;
  background: #ff7002;
}


.u-bg{
  position: relative;
  height: 180rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  background: #32343B;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
}

.top-wrap {
  height: 204rpx;
  position: relative;
  background: #ff7002 url(data:image/png;base64,) no-repeat right bottom /213rpx 121rpx;
}

.user-info {
  margin-top: 35rpx;
  margin-left: 30rpx;
  position: absolute;
  display: flex;
  width: 660rpx;
  align-items: center
}

.user-info .user-avatar {
  width: 116rpx;
  height: 116rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 7rpx solid #ffa767;
  flex-shrink: 0;
}

.user-info .user-avatar image {
  width: 100%;
  height: 100%;
  display: block;
}

.user-info .user-detail {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 16rpx;
  flex: 1
}

.user-info .user-detail .name {
  font-size: 34rpx;
  color: #fff;
}

.user-info .user-detail .id {
  font-size: 20rpx;
  color: #fff;
  margin-left: 20rpx;
}
.user-info .user-detail .my-flag{
  width: 132rpx;
  height: 34rpx;
  font-size: 20rpx;
  color: #fff;
  text-align: center;
  text-indent: 25rpx;
  line-height: 34rpx;
  margin-top: 10rpx;
  background: url(data:image/png;base64,) no-repeat center center/contain;
}
.user-info image{
  width: 144rpx;
  height: 118rpx;
}

.record-wrap {
  background-color: #fff;
  border-radius: 10rpx;
  display: flex;
  height: 125rpx;
  justify-content: center;
  align-items: center;
}

.record-wrap .item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.record-wrap .item .num {
  font-size: 34rpx;
  color: #393939;
}

.record-wrap .item .title {
  font-size: 24rpx;
  color: #999;
}

.settings-wrap {
  margin-top: 20rpx;
  background-color: #fff;
}
.mt-20{
  margin-top: 20rpx;
}
.settings-wrap .cell {
  height: 100rpx;
  border-bottom: 1rpx solid #f5f5f5;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  background: url(data:image/png;base64,) no-repeat center right 30rpx/13rpx 22rpx;
}

.settings-wrap .cell:active {
  background-color: #f5f5f5;
}
.settings-wrap .cell .icon-user-love image{
   width: 34rpx;
  height: 31rpx;
}
.settings-wrap .cell .icon-user-together image{
   width: 34rpx;
  height: 30rpx;
}
.settings-wrap .cell .icon-user-collect image{
  width: 35rpx;
  height: 34rpx;
}
.settings-wrap .cell .icon-user-info image {
  width: 34rpx;
  height: 32rpx;
}
.settings-wrap .cell .icon-user-security image{
  width: 31rpx;
  height: 35rpx;
}
.settings-wrap .cell .icon-user-settings image {
  width: 33rpx;
  height: 34rpx;
}

.settings-wrap .cell .icon-user-record image {
  width: 34rpx;
  height: 35rpx;
}

.settings-wrap .cell .icon-user-tickets image {
  width: 38rpx;
  height: 28rpx;
}

.settings-wrap .cell .icon-user-address image {
  width: 27rpx;
  height: 34rpx;
}

.settings-wrap .cell .icon {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 40rpx;
  height: 40rpx;
}

.settings-wrap .cell .icon image {
  display: block;
}

.settings-wrap .cell .title {
  font-size: 30rpx;
  color: #393939;
  margin-left: 25rpx;
}
