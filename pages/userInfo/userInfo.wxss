page {
  height: 100%;
   background-color: #fff;
}

.page {
  background-color: #f5f5f5;
}

.info-panel {
  background-color: #fff;
  height: 100%;
}

.user-info {
  padding-top: 70rpx;
  padding-bottom: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.user-info .avatar {
  width: 136rpx;
  height: 136rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 2rpx solid #eee;
  position: relative;
}

.user-info .avatar image {
  width: 100%;
  height: 100%;
  display: block;
}

.user-info .avatar .mask {
  background: rgba(0, 0, 0, 0.5) url(data:image/png;base64,) no-repeat center center/30rpx 30rpx;
  top: 0;
  left: 0;
  z-index: 100;
  width: 100%;
  height: 100%;
  position: absolute;
}

.user-info .detail .id {
  font-size: 24rpx;
  color: #999;
}

.user-setting{
  padding: 0 30rpx;
}
.user-setting .cell {
  margin-bottom: 30rpx;
}
.user-setting .cell .label{
  font-size: 28rpx;
  color: #393939;
  margin-bottom: 20rpx;
  display: block;
}
.user-setting .cell input{
  background-color: #f9f9f9;
  padding: 20rpx;
  font-size: 28rpx;
  color: #393939;
}
.user-setting .cell .sex-box{
  display: flex;
  justify-content: space-between;
}
.user-setting .cell .sex-box .box{
  width: 211rpx;
  height: 76rpx;
  border: 2rpx solid #e0e0e0;
  line-height: 76rpx;
text-align: center;
font-size: 28rpx;
color: #888;
}
.user-setting .cell .sex-box .box.s-cur{
  border-color: #666;
}
.user-setting .cell picker{
  border: 2rpx solid #e0e0e0;
  height: 76rpx;
  line-height: 76rpx;
  background: url(data:img/jpg;base64,) no-repeat center right 24rpx/25rpx 17rpx;
}
.user-setting .cell .pickVal{
  color: #888888;
  font-size: 28rpx;
  padding-left: 18rpx;
}

.query-btn{
  width: 680rpx;
  height: 90rpx;
  margin: 0 auto;
  border-radius:45rpx;
  border: none;
  background-color: #f8592d;
  color: #fff; 
  font-size: 32rpx;
  line-height: 90rpx;
  text-align: center;
}