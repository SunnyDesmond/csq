page {
  height: 100%;
}

.page {
  height: 100%;
}

.banner {
  background-color: #efefef;
}

.banner swiper {
  height: 100%;
}

.banner image {
  width: 100%;
  height: 100%;
}

.fast-nav {
  display: flex;
  justify-content: space-around;
  align-items: center;
  height:240rpx;
  padding-top:56rpx
}

.fast-nav .item {
  width: 30%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.fast-nav .item .item-icon {
  width: 86rpx;
  height: 86rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fb581e;
}

.fast-nav .item image {
  width: 56rpx;
  height: 56rpx;
}

.fast-nav .item text {
  font-size: 28rpx;
  padding-top: 20rpx;
  color: #393939;
}
